    "scripts": {
        "i18n:extract": "yarn formatjs extract 'src/{**,**/**,**/**/**,**/**/**/**,**/**/**/**/**}*.tsx' --extract-source-location true --id-interpolation-pattern '[sha512:contenthash:base64:6]' --throws --out-file extracted-messages/en.json",
        "i18n:compile": "yarn formatjs compile extracted-messages/{ru,en}.json --out-file src/translations/{ru,en}.json",
        "i18n": "yarn i18n:extract && yarn i18n:compile",
        "build:client:dev": "yarn webpack --progress --color --config webpack/client/webpack-config-dev.ts",
        "build:client:prod": "yarn webpack --progress --color --config webpack/client/webpack-config-prod.ts",
        "build:server:prod": "yarn webpack --progress --color --config webpack/server/webpack-config-prod.ts",
        "build:server:dev": "yarn webpack --progress --color --config webpack/server/webpack-config-dev.ts",
        "rebuild-binary": "yarn add --force jpegtran",
        "analyze": "cross-env ANALYZE=analyze yarn build:client:prod",
        "build:all:prod": "yarn build:client:prod && yarn build:server:prod",
        "start:server:dev": "yarn build:server:dev --watch",
        "start:server:prod": "yarn build:all:prod && yarn node ./dist",
        "start": "yarn start:dev",
        "start:dev": "yarn build:client:dev && yarn webpack serve --progress --color --config webpack/client/webpack-config-dev.ts",
        "start:prod": "yarn build:all:prod && yarn node ./dist",
        "build:dist": "yarn build:all:prod",
        "lint": "eslint --fix --ext .ts,.tsx ./src/"
    },
